#include <iostream>

int main()
{
    printf("+--------+---------+----------+\r\n");
    printf("| Length | 60 km/h | 110 km/h |\r\n");
    printf("+--------+---------+----------+\r\n");


    for(int wayLength = 50; wayLength < 501; wayLength += 50)
    {

        printf("| %5d | %.5f | %.5f |\r\n",
               wayLength,
               8.2 / 100.0 * wayLength,
               7.1 / 100.0 * wayLength);
        printf("+--------+---------+----------+\r\n");
    }

    return 0;
}
